    <title>@ViewBag.Title</title>
    <link rel="apple-touch-icon" sizes="76x76" href="~/burak/admin/assets/img/logoo.png">
    <link rel="icon" type="image/png" href="~/burak/admin/assets/img/favicon/favicon-32x32.png">
    <title>
        by Orange Team
    </title>
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900|Roboto+Slab:400,700" />
    <!-- Nucleo Icons -->
    <link href="~/burak/admin/assets/css/nucleo-svg.css" rel="stylesheet" />
    <link href="~/burak/admin/assets/css/material-dashboard.css" rel="stylesheet" />
    <link href="~/burak/admin/assets/css/nucleo-icons.css" rel="stylesheet" />
